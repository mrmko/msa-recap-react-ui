{
  "scriptFile": "__init__.py",
  "bindings": [ 
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "jsonBlob",
      "type": "blob",
      "dataType": "binary",
      "path": "insights/{Insights.FileName}",
      "connection": "MyStorageConnectionAppSetting",
      "direction": "in"
    }
  ]
}